#echo [.environ]
# common settings between .xsession and .profile (and .bash_profile)

. /etc/profile.d/environ.sh

## on-time evaluation begins
test "$environ" || {
export environ; environ=1

[ "$TERM" = ansisys ] && TERM=cygwin	# for cmd ?
export MAKE_MODE; MAKE_MODE=unix
export SHELL
# Any other envvars are set in Windows' "System's Property" dialog.

}
# end of one time evaluation
